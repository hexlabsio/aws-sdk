{
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "outDir": "./dist2",
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": false,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "allowJs": true,
    "declaration": true,
    "moduleResolution": "node",
    "target": "ESNext",
    "pretty": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "types": [],
    "esModuleInterop": true
  },
  "include": [
    "generator.ts"
  ],
  "exclude": ["node_modules"]
}
